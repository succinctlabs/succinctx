{
    "rust-analyzer.linkedProjects": [
        "./plonky2x/Cargo.toml",
    ],
    "rust-analyzer.check.overrideCommand": [
        "cargo",
        "clippy",
        "--workspace",
        "--message-format=json",
        "--all-features",
        "--all-targets",
        "--",
        "-D",
        "warnings",
        "-A",
        "incomplete-features"
    ],
    "rust-analyzer.runnables.extraEnv": {
        "RUST_LOG": "debug",
        "RUSTFLAGS": "-Ctarget-cpu=native"
    },
    "rust-analyzer.runnables.extraArgs": [
        "--release"
    ],
}